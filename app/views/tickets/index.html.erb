<p id="notice"><%= notice %></p>

<h1>Tickets</h1>

<table>
  <thead>
    <tr>
      <th>Ticket</th>
      <th>Subject</th>
      <th>Description</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tickets.each do |t| %>
      <tr>
        <td><%= t.ticket_number %></td>
        <td><%= t.subject %></td>
        <td><%= t.description %></td>
        <td><%= t.user_id %></td>
        <td><%= link_to 'Show', tickets_path %></td>
        <td><%= link_to 'Edit', edit_ticket_path(t) %></td>
        <td><%= link_to 'Destroy', t, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket', new_ticket_path %>
